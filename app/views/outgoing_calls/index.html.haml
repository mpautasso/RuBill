%h1 Listing outgoing calls

= form_tag outgoing_calls_path, :method => 'get', :id => "outgoingcalls_search" do
  %p
    = text_field_tag :search, params[:search], :placeholder => 'Enter the call date to search...'
    #submit
      .button
        = submit_tag "Search", :name => nil
  %p
  #outgoing_calls_table
    = render 'outgoing_calls'
