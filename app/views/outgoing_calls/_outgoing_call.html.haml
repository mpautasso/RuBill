%tr{:id => "outgoingcall_#{outgoing_call.id}", :class => "body #{cycle('odd','even')}"}
  %td= l outgoing_call.calldate, :format => :human
  %td= outgoing_call.src
  %td= outgoing_call.dst
  %td= outgoing_call.duration
  %td= outgoing_call.billsec
  %td= number_to_currency(outgoing_call.cost)
  - if current_user.admin?
    %td= link_to image_tag("my_images/edit.png", :alt => "Edit"), edit_outgoing_call_path(outgoing_call), :class => 'popme'
    %td= link_to image_tag("my_images/delete.png", :alt => "Delete"), outgoing_call, :confirm => t(:are_sure), |
                              :method => :delete, |
                              :remote => true
  - else
    %td
