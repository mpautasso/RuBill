= form_for @csv_file do |f|
  -if @csv_file.errors.any?
    #error_explanation
      %h2= "#{pluralize(@csv_file.errors.count, "error")} prohibited this csv_file from being saved:"
      %ul
        - @csv_file.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :file_name
    = f.file_field :file_name

  .actions
    = f.submit 'Save'
